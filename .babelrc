{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-modern-regexp",
    "babel-plugin-styled-components",
    "./scripts/babel-css-modules.js"
  ],
  "env": {
    "production": {
      "ignore": [
        "./components/**/*.test.jsx",
        "./components/**/*/__snapshots__"
      ]
    }
  }
}
